<style>

</style>

<div id="ticket_list_pane">
    <div class="row" style="padding: 3vw;">


        <div class="col l12  s12 m12">

            <center>

                <h4>Tickets &nbsp;&nbsp;&nbsp; <a class="btn-floating btn-large waves-effect waves-light red"><i
                                class="material-icons"
                                @click="download_csv">print</i></a></h4>
                <div v-if="is_no_data">
                    <img style="margin-top: 10vw;"
                         src="data:image/png;base64,">
                    <h5> No data found</h5>
                </div>
            </center>
            <transition name="slide">
                <div v-if="!is_no_data">
                    <br>
                    <table class="w3-responsive w3-table">
                        <thead class="w3-hover-blue">
                        <tr>
                            <th>Actions</th>
                            <th v-for="key in ticket_keys">{{key}}</th>
                        </tr>

                        </thead>

                        <tbody >
                        <tr class="w3-hover-blue-gray" v-for="ticket in tickets_list">
                            <td>
                                <a class="blue-text" v-if="!ticket.payment_done"
                                   @click="confirm_payment(ticket.h_id)">
                                    Confirm payment
                                </a>
                                <a class="red-text" v-if="ticket.payment_done"
                                   @click="deconfirm_payment(ticket.h_id)">
                                    De-Confirm payment
                                </a>
                            </td>
                            <td v-for="key in ticket_keys">{{ticket[key]}}</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </transition>

        </div>

    </div>
</div>

<script>

    let ticket_list_pane = new Vue({
        el: '#ticket_list_pane',
        data: {
            ticket_keys: [],
            tickets_list: [],
            is_no_data: true
        },
        methods: {
            do_after_load: function () {
                this.get_tickets_list();

            },
            get_tickets_list: function () {

                axios.post('/dashboard/select/tickets/10000/0')
                    .then(function (response) {
                        if (response.data.results.length > 0) {
                            for (let i = 0; i < response.data.results.length; i++) {
                                let data = {}
                                data.event_name = response.data.results[i].eventsByeventHId.name;
                                data.email = response.data.results[i].usersByuserHId.email;
                                data.name = response.data.results[i].usersByuserHId.name;
                                data.phone_number = response.data.results[i].usersByuserHId.phone_number;
                                data.dob = response.data.results[i].usersByuserHId.dob;
                                data.standard = response.data.results[i].usersByuserHId.standard;
                                data.school_name = response.data.results[i].usersByuserHId.school_name;
                                data.school_city = response.data.results[i].usersByuserHId.school_city;
                                data.created_at = response.data.results[i].created_at;
                                data.event_price = response.data.results[i].eventsByeventHId.price;
                                data.payment_done = response.data.results[i].payment_done;
                                data.h_id = response.data.results[i].h_id;
                                data.user_h_id = response.data.results[i].user_h_id;
                                data.event_h_id = response.data.results[i].event_h_id;

                                ticket_list_pane.tickets_list.push(data)
                                ticket_list_pane.ticket_keys = Object.keys(data);
                            }
                            ticket_list_pane.is_no_data = false;
                        } else {
                            ticket_list_pane.is_no_data = true;
                        }
                    })
                    .catch(function (error) {
                        console.log(error);
                    });
            },
            convert_to_csv: function (objArray) {
                let array = typeof objArray != 'object' ? JSON.parse(objArray) : objArray;
                let str = '';
                for (let i = 0; i < array.length; i++) {
                    let line = '';
                    for (let index in array[i]) {
                        if (line != '') line += ','

                        line += array[i][index];
                    }
                    str += line + '\r\n';
                }
                return str;
            },
            download_csv: function () {
                let element = document.createElement('a');
                element.setAttribute('href', 'data:text/plain;charset=utf-8,' + encodeURIComponent(this.convert_to_csv(this.tickets_list.filter(x => x.payment_done === true))));
                element.setAttribute('download', "ticket.csv");

                element.style.display = 'none';
                document.body.appendChild(element);

                element.click();

                document.body.removeChild(element);
            },
            confirm_payment: function (h_id) {
                for (let i = 0; i < ticket_list_pane.tickets_list.length; i++) {
                    if (ticket_list_pane.tickets_list[i].h_id === h_id) {
                        ticket_list_pane.tickets_list[i].payment_done = true;
                        axios.post('/dashboard/edit/ticket/confirm_payment/true/', {
                            h_id: h_id,
                            user_h_id: ticket_list_pane.tickets_list[i].user_h_id,
                            event_h_id: ticket_list_pane.tickets_list[i].event_h_id
                        })
                            .then(function (response) {
                                if (response.data.RESPONSE_CODE === 108200) {
                                    M.toast({html: '<span style="color: lightgreen; ">Confirmed</span>'});
                                }
                            })
                            .catch(function (error) {
                                console.log(error);
                            });
                        break;
                    }
                }
            },
            deconfirm_payment: function (h_id) {
                for (let i = 0; i < ticket_list_pane.tickets_list.length; i++) {
                    if (ticket_list_pane.tickets_list[i].h_id === h_id) {
                        ticket_list_pane.tickets_list[i].payment_done = false;
                        axios.post('/dashboard/edit/ticket/confirm_payment/false/', {
                            h_id: h_id,
                            user_h_id: ticket_list_pane.tickets_list[i].user_h_id,
                            event_h_id: ticket_list_pane.tickets_list[i].event_h_id
                        })
                            .then(function (response) {
                                if (response.data.RESPONSE_CODE === 108200) {
                                    M.toast({html: '<span style="color: lightcoral; ">Revoked Confirmation</span>'});
                                }
                            })
                            .catch(function (error) {
                                console.log(error);
                            });
                        break;
                    }
                }

            }
        }, mounted: function () {
            this.$nextTick(function () {
                this.do_after_load();
            })
        }
    });


</script>